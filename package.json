{
  "name": "household_finance",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "build": "npm run prettier-format && tsc",
    "start": "npm run build && node ./build/index.js",
    "dev": "ts-node ./build/index.js",
    "lint": "npm run prettier-format && eslint . --ext .ts",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' 'index.ts' --write",
    "prettier-watch": "onchange 'src/**/*.ts' -- prettier --write {{changed}}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "onchange": "^7.1.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "body-parser": "^1.19.2",
    "express": "^4.17.3",
    "winston": "^3.6.0"
  },
  "description": "",
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run tsc"
    }
  }
}
